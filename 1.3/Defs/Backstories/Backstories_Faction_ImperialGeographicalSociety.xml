	<!-- Writer -->

	<AlienRace.BackstoryDef>
		<defName>ESCP_Imperial_ImperialGeographicalSocietyWriterBackstory</defName>
		<title>Imperial Geographical Society Writer</title>
		<titleShort>I.G.S. Writer</titleShort>
		<baseDescription>[PAWN_nameDef] is a writer for the Imperial Geographical Society. This typically involves writing guides to differing areas within the Empire as well as different points of view for traveling there be it as a noble or commoner.</baseDescription>
		<slot>Adulthood</slot>
		<bodyTypeMale>Male</bodyTypeMale>
		<bodyTypeFemale>Female</bodyTypeFemale>
		<spawnCategories>
		  <li>ESCP_ImperialBackstory</li>
		</spawnCategories>
		<skillGains>
			<li>
				<defName>Intellectual</defName>
				<amount>3</amount>
			</li>
		</skillGains>
		<forcedTraits>
			<li>
			  <defName>ESCP_Imperial_Culture</defName>
				<degree>3</degree>
				<chance>5</chance>
			</li>
			<li>
			  <defName>ESCP_Imperial_Culture</defName>
				<degree>4</degree>
				<chance>25</chance>
			</li>
			<li>
			  <defName>ESCP_Imperial_Culture</defName>
				<degree>2</degree>
				<chance>100</chance>
			</li>
		</forcedTraits>
		<disallowedTraits>
		</disallowedTraits>
		<workDisables>
		</workDisables>
		<maleCommonality>45</maleCommonality>
		<femaleCommonality>45</femaleCommonality>
	</AlienRace.BackstoryDef>

